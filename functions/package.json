{
    "name": "functions",
    "description": "Cloud Functions for Firebase",
    "scripts": {
        "lint": "eslint .",
        "serve": "export GOOGLE_APPLICATION_CREDENTIALS=key.json && firebase emulators:start --only functions",
        "shell": "firebase functions:shell",
        "start": "npm run shell",
        "deploy": "firebase deploy --only functions",
        "logs": "firebase functions:log"
    },
    "engines": {
        "node": "10"
    },
    "dependencies": {
        "@google-cloud/storage": "^5.0.1",
        "body-parser": "^1.19.0",
        "busboy": "^0.3.1",
        "express": "^4.17.1",
        "firebase-admin": "^8.6.0",
        "firebase-functions": "^3.3.0",
        "fs-extra": "^9.0.1",
        "sharp": "^0.25.4",
        "uuidv4": "^6.1.0"
    },
    "devDependencies": {
        "eslint": "^5.12.0",
        "eslint-plugin-promise": "^4.0.1",
        "firebase-functions-test": "^0.1.6"
    },
    "private": true
}
